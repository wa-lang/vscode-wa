{
  "name": "vscode-wa",
  "displayName": "Wa",
  "version": "0.0.2",
  "description": "Wa Lang for VS Code",
  "publisher": "xxxDeveloper",
  "homepage": "https://github.com/wa-lang/vscode-wa#readme",
  "bugs": {
    "url": "https://github.com/wa-lang/vscode-wa/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wa-lang/vscode-wa"
  },
  "main": "./dist/extension.js",
  "files": [
    "dist"
  ],
  "activationEvents": [
    "*"
  ],
  "sideEffects": false,
  "engines": {
    "vscode": "^1.66.2"
  },
  "contributes": {
    "languages": [
      {
        "id": "wa",
        "extensions": [
          ".wa"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./assets/wa.svg",
          "dark": "./assets/wa.svg"
        }
      }
    ],
    "grammars": [
      {
        "language": "wa",
        "scopeName": "source.wa",
        "path": "./syntaxes/wa.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "wa",
        "path": "./snippets/snippets.json"
      }
    ],
    "commands": [],
    "keybindings": [],
    "configuration": {
      "properties": {}
    }
  },
  "icon": "assets/logo.png",
  "scripts": {
    "dev": "rm -rf dist && tsup --watch",
    "build": "tsup",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publish": "vsce publish --no-dependencies --allow-star-activation",
    "pack": "vsce package --no-dependencies --allow-star-activation"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.21.1",
    "@types/node": "^17.0.29",
    "@types/vscode": "^1.66.0",
    "eslint": "^8.14.0",
    "tsup": "^5.12.6",
    "typescript": "^4.6.3",
    "vite": "^2.9.6",
    "vsce": "^2.7.0"
  }
}
